{
  "name": "foreslash",
  "version": "0.3.11",
  "description": "Foreslash is a Javascript utilities lib which contains plenty of practical functions.",
  "author": "moushu",
  "license": "Mulan PSL v2",
  "bugs": {
    "url": "https://github.com/Moushudyx/foreslash/issues"
  },
  "homepage": "https://github.com/Moushudyx/foreslash#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/Moushudyx/foreslash.git"
  },
  "type": "module",
  "main": "lib/index.mjs",
  "module": "lib/index.mjs",
  "browser": "lib/index.umd.js",
  "types": "lib/index.d.ts",
  "exports": {
    ".": {
      "import": "./lib/index.mjs",
      "require": "./lib/index.cmn.cjs",
      "types": "./lib/index.d.ts"
    },
    "./umd": "./lib/index.umd.js"
  },
  "files": [
    "lib",
    "skills",
    "README.EN.md",
    "CHANGELOG.md",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "dev": "rolldown -c -w",
    "build": "rolldown -c",
    "test": "vitest run --coverage",
    "test:slow": "vitest run --coverage --no-threads",
    "prepublishOnly": "pnpm test && pnpm run build",
    "docs:dev": "rolldown -c && vitepress dev docs",
    "docs:build": "rolldown -c && vitepress build docs",
    "docs:preview": "rolldown -c && vitepress preview docs"
  },
  "keywords": [
    "foreslash",
    "lib",
    "util",
    "typescript",
    "functional"
  ],
  "devDependencies": {
    "@rollup/plugin-typescript": "^12.1.2",
    "@vitest/coverage-v8": "^3.2.4",
    "jsdom": "^24.1.3",
    "rolldown": "^1.0.0-rc.1",
    "rollup": "^4.38.0",
    "rollup-plugin-dts": "^6.2.1",
    "sass-embedded": "^1.90.0",
    "tslib": "^2.7.0",
    "typescript": "^5.5.4",
    "vitepress": "^1.6.3",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "ts-toolbelt": "^9.6.0"
  }
}
