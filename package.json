{
  "name": "foreslash",
  "version": "0.1.1",
  "description": "Foreslash is a Javascript utilities lib which contains plenty of practical functions.",
  "author": "moushu",
  "license": "Mulan PSL v2",
  "bugs": {
    "url": "https://github.com/Moushudyx/foreslash/issues"
  },
  "homepage": "https://github.com/Moushudyx/foreslash#readme",
  "repository": {
    "type": "git",
    "url": "git://github.com/Moushudyx/foreslash.git"
  },
  "type": "module",
  "main": "lib/index.mjs",
  "module": "lib/index.mjs",
  "browser": "lib/index.umd.js",
  "types": "lib/index.d.ts",
  "exports": {
    ".": {
      "import": "./lib/index.mjs",
      "require": "./lib/index.cmn.cjs"
    },
    "./umd": "./lib/index.umd.js"
  },
  "files": [
    "lib",
    "README.md"
  ],
  "scripts": {
    "dev": "rollup -c -w",
    "build": "rollup -c",
    "test": "jest --coverage",
    "prepublishOnly": "npm run build && npm run test",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "keywords": [
    "foreslash",
    "lib",
    "util",
    "utilities",
    "typescript",
    "functional",
    "modules"
  ],
  "devDependencies": {
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/jest": "^29.5.12",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "rollup": "^3.29.4",
    "rollup-plugin-dts": "^6.1.1",
    "ts-jest": "^29.2.5",
    "tslib": "^2.7.0",
    "typescript": "^5.5.4",
    "vitepress": "^1.3.4",
    "yarn": "^1.22.22"
  },
  "dependencies": {
    "ts-toolbelt": "^9.6.0"
  }
}
