{
  "name": "foreslash",
  "version": "0.3.7",
  "description": "Foreslash is a Javascript utilities lib which contains plenty of practical functions.",
  "author": "moushu",
  "license": "Mulan PSL v2",
  "bugs": {
    "url": "https://github.com/Moushudyx/foreslash/issues"
  },
  "homepage": "https://github.com/Moushudyx/foreslash#readme",
  "repository": {
    "type": "git",
    "url": "git://github.com/Moushudyx/foreslash.git"
  },
  "type": "module",
  "main": "lib/index.mjs",
  "module": "lib/index.mjs",
  "browser": "lib/index.umd.js",
  "types": "lib/index.d.ts",
  "exports": {
    ".": {
      "import": "./lib/index.mjs",
      "require": "./lib/index.cmn.cjs",
      "types": "./lib/index.d.ts"
    },
    "./umd": "./lib/index.umd.js"
  },
  "files": [
    "lib",
    "README.EN.md",
    "CHANGELOG.md",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "dev": "rollup -c -w",
    "build": "rollup -c",
    "tsd": "tsd --files /test/**/*.test-d.ts",
    "test": "jest --coverage",
    "test:slow": "jest --coverage --runInBand",
    "prepublishOnly": "jest --coverage && npm run build",
    "docs:dev": "rollup -c && vitepress dev docs",
    "docs:build": "rollup -c && vitepress build docs",
    "docs:preview": "rollup -c && vitepress preview docs"
  },
  "keywords": [
    "foreslash",
    "lib",
    "util",
    "typescript",
    "functional"
  ],
  "devDependencies": {
    "@rollup/plugin-typescript": "^12.1.2",
    "@types/jest": "^29.5.12",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "rollup": "^4.38.0",
    "rollup-plugin-dts": "^6.2.1",
    "sass-embedded": "^1.90.0",
    "ts-jest": "^29.2.5",
    "tsd": "^0.33.0",
    "tslib": "^2.7.0",
    "typescript": "^5.5.4",
    "vitepress": "^1.6.3",
    "yarn": "^1.22.22"
  },
  "dependencies": {
    "ts-toolbelt": "^9.6.0"
  }
}
